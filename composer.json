{
    "name": "generationtux/healthz",
    "description": "Health checks for PHP apps.",
    "license": "MIT",
    "authors": [
        {
            "name": "Kyle Ferguson",
            "email": "kyle.ferguson@generationtux.com"
        },
        {
          "name": "Damien Russell",
          "email": "christopher.russell@generationtux.com"
        },
        {
          "name": "Thomas Manley",
          "email": "thomas.manley@generationtux.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Gentux\\Healthz\\": "src/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "aws/aws-sdk-php": "~3.0",
        "illuminate/contracts": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/queue": "^9.0|^10.0|^11.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "^1.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gentux\\Healthz\\Support\\HealthzServiceProvider"
            ]
        }
    }
}
